{
  "name": "Feedback",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": false
  },
  "http": {
    "path": "feedbacks"
  },
  "properties": {
    "value": {
      "type": "object"
    },
    "updatedAt": {
      "type": "date"
    },
    "sentAt": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {},
  "mixins": {
    "TimeStamp" : true
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
