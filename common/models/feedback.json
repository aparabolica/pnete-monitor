{
  "name": "Feedback",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": false
  },
  "http": {
    "path": "feedbacks"
  },
  "properties": {
    "value": {
      "type": "object"
    },
    "updatedAt": {
      "type": "date"
    },
    "sentAt": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "organization": {
      "type": "belongsTo",
      "model": "Organization",
      "foreignKey": ""
    },
    "cycle": {
      "type": "belongsTo",
      "model": "Cycle",
      "foreignKey": ""
    },
    "indicator": {
      "type": "belongsTo",
      "model": "Indicator",
      "foreignKey": ""
    }
  },
  "mixins": {
    "TimeStamp" : true
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "udpate"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
