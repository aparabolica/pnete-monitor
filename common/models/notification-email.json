{
  "name": "NotificationEmail",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": false
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "status": {
      "type": "string",
      "required": true,
      "default": "pending"
    }
  },
  "validations": [],
  "relations": {
    "task": {
      "type": "belongsTo",
      "model": "NotificationTask",
      "foreignKey": "taskId"
    },
    "organization": {
      "type": "belongsTo",
      "model": "Organization",
      "foreignKey": ""
    },
    "recipient": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
